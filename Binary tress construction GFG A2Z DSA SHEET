class Solution:
    def createTree(self, root, l):
        # Code here
        root1 = Node(l[1])
        root2 = Node(l[2])
        root3 = Node(l[3])
        root4 = Node(l[4])
        root5 = Node(l[5])
        root6 = Node(l[6])
        
        root.left = root1
        root.right = root2
        root1.left = root3
        root1.right = root4
        root2.left = root5
        root2.right = root6
        
        
